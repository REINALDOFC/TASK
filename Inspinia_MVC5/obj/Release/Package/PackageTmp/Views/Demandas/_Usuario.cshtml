
 
<dl class="row mb-0">
    <div class="col-sm-12 text-sm-left" id="divListaUsuarioTarefa">
        <dd class="project-people mb-1" id="dd_ListaUsuario_tarefa">
            @foreach (var item in ViewBag.usuario)
            {
                <a href="@Url.Action("demandas_usuario", "demandas")?id=@item.Usu_Nome" class="float-left" style="padding-left:5px;"><img alt="image" class="rounded-circle" src="~/Images/@item.Usu_Foto"></a>

            }
        </dd>

    </div>
</dl>
       
   
